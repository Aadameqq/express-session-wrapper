{
  "name": "express-session-wrapper",
  "version": "1.0.1",
  "description": "A package that extends the functionality of the express-session package",
  "types": "./dist/cjs/types/index.d.ts",
  "main": "./dist/cjs/index.js",
  "scripts": {
    "compile:mjs": "tsc -p ./tsconfig.mjs.json",
    "compile:cjs": "tsc -p ./tsconfig.cjs.json",
    "build": "npm run compile:cjs && npm run compile:mjs",
    "prepack": "npm run build",
    "lint": "npx eslint src/**/*.ts",
    "test": "jest --passWithNoTests",
    "prettier-check": "npx prettier --check src"
  },
  "keywords": [
    "session",
    "express-session",
    "session-wrapper"
  ],
  "author": "https://github.com/Aadameqq",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2"
  },
  "devDependencies": {
    "@babel/core": "^7.21.0",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.21.0",
    "@types/express": "^4.17.17",
    "@types/express-session": "^1.17.7",
    "@types/jest": "^29.4.0",
    "@types/node": "^18.13.0",
    "@typescript-eslint/eslint-plugin": "^5.53.0",
    "@typescript-eslint/parser": "^5.54.1",
    "babel-jest": "^29.4.3",
    "eslint": "^8.34.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.4.3",
    "prettier": "2.8.4",
    "typescript": "^4.9.5"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/esm/types/index.d.ts",
        "default": "./dist/esm/index.mjs"
      },
      "require": {
        "types": "./dist/cjs/types/index.d.ts",
        "default": "./dist/cjs/index.js"
      }
    }
  },
  "files": [
    "dist/**/*"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Aadameqq/express-session-wrapper.git"
  }
}
